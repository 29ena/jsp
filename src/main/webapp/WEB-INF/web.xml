<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>jsp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>ADMIN</param-name>
    <param-value>brown</param-value>
  </context-param>
  <servlet>
    <servlet-name>basicServlet</servlet-name>
    <servlet-class>ddit.servlet.BasicServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>basicServlet</servlet-name>
    <url-pattern>/bs</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>nowDateServlet</servlet-name>
    <servlet-class>ddit.servlet.NowDateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>nowDateServlet</servlet-name>
    <url-pattern>/nowDate</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>timesTablesServlet</servlet-name>
    <servlet-class>ddit.servlet.TimesTablesServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>timesTablesServlet</servlet-name>
    <url-pattern>/timesTablesServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LoginController</display-name>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>kr.or.ddit.login.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SumCalculation</display-name>
    <servlet-name>SumCalculation</servlet-name>
    <servlet-class>kr.or.ddit.sumCalculation.SumCalculation</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SumCalculation</servlet-name>
    <url-pattern>/SumCalculation</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MulCalculation</display-name>
    <servlet-name>MulCalculation</servlet-name>
    <servlet-class>kr.or.ddit.mulCalculation.MulCalculation</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MulCalculation</servlet-name>
    <url-pattern>/MulCalculation</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>UserController</display-name>
    <servlet-name>UserController</servlet-name>
    <servlet-class>kr.or.ddit.user.controller.UserController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserController</servlet-name>
    <url-pattern>/UserController</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/error/404/404.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/error/500/500.jsp</location>
  </error-page>
  <error-page>
    <exception-type>java.lang.NumberFormatException</exception-type>
    <location>/WEB-INF/error/numberFormatException.jsp</location>
  </error-page>
  <filter>
    <filter-name>loggingFilter2</filter-name>
    <filter-class>kr.or.ddit.filter.LoggingFilter2</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>loggingFilter2</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>loggingFilter</filter-name>
    <filter-class>kr.or.ddit.filter.LoggingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>loggingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>requestParameterEncodingFilter</filter-name>
    <filter-class>kr.or.ddit.filter.RequestParameterEncodingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>requestParameterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>DefaultParamFilter</filter-name>
    <filter-class>kr.or.ddit.filter.DefaultParamFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>DefaultParamFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>kr.or.ddit.listener.SessionListener</listener-class>
  </listener>
  <listener>
    <listener-class>kr.or.ddit.listener.SessionAttributeListener</listener-class>
  </listener>
  <listener>
    <listener-class>kr.or.ddit.listener.ApplicationListener</listener-class>
  </listener>
</web-app>